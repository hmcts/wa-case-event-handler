{
  "connectionString": "${file:/mnt/secrets/wa/app-insights-connection-string}",
  "role": {
    "name": "WA Case Event Handler"
  },
  "sampling": {
    "percentage": 100
  },
  "preview": {
    "sampling": {
      "overrides": [
        {
          "telemetryKind": "request",
          "attributes": [
            {
              "key": "http.url",
              "value": "https?://[^/]+/health.*",
              "matchType": "regexp"
            }
          ],
          "percentage": 1
        },
        {
          "telemetryKind": "exception",
          "attributes": [
            {
              "key": "LoggerName",
              "value": ".*CcdCaseEventsConsumer.*",
              "matchType": "regexp"
            }
          ],
          "percentage": 0
        },
        {
          "telemetryKind": "exception",
          "attributes": [
            {
              "key": "Logger Message",
              "value": "Error occurred while receiving messages",
              "matchType": "strict"
            }
          ],
          "percentage": 0
        },
        {
          "telemetryKind": "exception",
          "attributes": [
            {
              "key": "Logger Message",
              "value": "Error occurred while closing the session",
              "matchType": "strict"
            }
          ],
          "percentage": 0
        },
        {
          "telemetryKind": "exception",
          "attributes": [
            {
              "key": "LoggerName",
              "value": ".*CcdCaseEventsDeadLetterQueueConsumer.*",
              "matchType": "regexp"
            }
          ],
          "percentage": 0
        },
        {
          "telemetryKind": "exception",
          "attributes": [
            {
              "key": "Logger Message",
              "value": "Error occurred while completing the message processing",
              "matchType": "strict"
            }
          ],
          "percentage": 0
        }
      ]
    }
  }
}
