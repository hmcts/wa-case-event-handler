{
  "connectionString": "${file:/mnt/secrets/wa/app-insights-connection-string}",
  "role": {
    "name": "WA Case Event Handler"
  },
  "sampling": {
    "percentage": 100
  },
  "preview": {
    "sampling": {
      "overrides": [
        {
          "telemetryType": "request",
          "attributes": [
            {
              "key": "http.url",
              "value": "https?://[^/]+/health.*",
              "matchType": "regexp"
            }
          ],
          "percentage": 1
        },
        {
          "telemetryType": "exception",
          "attributes": [
            {
              "key": "LoggerName",
              "value": "CcdCaseEventsConsumer",
              "matchType": "regexp"
            }
          ],
          "percentage": 1
        }
      ]
    }
  }
}
